datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model CommentAnalysis {
  analysisResult Json?
  commentId      String?
  createdAt      DateTime @default(now())
  id             String   @id @default(cuid())
  updatedAt      DateTime @updatedAt
}
